{
  "name": "thefossilhound",
  "version": "1.0.1",
  "private": true,
  "description": "",
  "files": [
    ".npmrc",
    "index.js",
    "package-lock.json"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production next build src/app",
    "export": "dotenv -e .env -e .env-secure next export src/app",
    "dev": "nodemon -w src/server -r esm src/server/index.js",
    "precommit": "cross-env NODE_ICU_DATA=node_modules/full-icu lint-staged",
    "start": "cross-env NODE_ENV=production node --icu-data-dir=node_modules/full-icu -r esm src/server/index.js",
    "storybook": "npx cross-env NODE_ENV=development IS_STORYBOOK=true npx start-storybook -p 9902 -c .storybook -s src/app/static",
    "verify": "npm run lint && npm run test",
    "lint": "eslint .",
    "test": "node --icu-data-dir=node_modules/full-icu node_modules/jest/bin/jest.js --no-cache --detectOpenHandles --config jest.config.js",
    "watch:test": "npm test -- --watch",
    "format": "prettier --write '**/*.{js,json,md}'"
  },
  "keywords": [],
  "license": "MIT",
